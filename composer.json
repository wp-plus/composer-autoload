{
    "name": "wp-plus/composer-autoload",
    "description": "Automagically require the autoload.php generated by Composer (as a must-use plugin).",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^2.3",
        "php": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WpPlus\\ComposerAutoload\\": "src/"
        }
    },
    "extra": {
        "class": "WpPlus\\ComposerAutoload\\Plugin"
    },
    "require-dev": {
        "composer/composer": "^2.7"
    }
}
